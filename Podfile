platform :ios, '9.0'

target 'Employed' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Employed
  
  # Networking
  pod 'Networking'
  
  # Presentr
  pod 'Presentr'
  
  # Swipeable Card-Views
  pod 'Koloda'
  
  # Swift Protobufs
  pod 'SwiftProtobuf'
  
  # Tagview
  pod 'TagListView', '~> 1.0'
  
  # MessageKit
  pod 'MessageKit'
  
  # Color Scheme
  pod 'ChameleonFramework/Swift', :git => 'https://github.com/ViccAlexander/Chameleon.git'
  
end
